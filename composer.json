{
    "name": "elfsundae/httpclient",
    "type": "library",
    "description": "HttpClient is a smart Guzzle wrapper provides convenient method chaining, global request options, and magic methods to customize any request options.",
    "keywords": ["http", "client", "guzzle", "chaining"],
    "homepage": "https://github.com/ElfSundae/httpclient",
    "license": "MIT",
    "authors": [
        {
            "name": "Elf Sundae",
            "email": "elf.sundae@gmail.com",
            "homepage": "https://0x123.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "~6.3",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.7|~6.0"
    },
    "autoload": {
        "psr-4": {
            "ElfSundae\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
