{
    "name": "elfsundae/httpclient",
    "type": "library",
    "description": "A smart Guzzle wrapper provides convenient method chaining, global request options, and magic methods to customize request options.",
    "keywords": ["http", "client", "guzzle", "chaining"],
    "homepage": "https://github.com/ElfSundae/httpclient",
    "license": "MIT",
    "authors": [
        {
            "name": "Elf Sundae",
            "email": "elf.sundae@gmail.com",
            "homepage": "https://0x123.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "~6.3|~7.0",
        "illuminate/support": "~5.0|~6.0|~7.0|~8.0|~9.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.7|~6.0|~7.0|~8.0|~9.0",
        "dms/phpunit-arraysubset-asserts": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "ElfSundae\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
