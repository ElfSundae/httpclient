{
    "name": "elfsundae/httpclient",
    "type": "library",
    "description": "A wrapper of Guzzle HTTP client.",
    "keywords": ["httpclient"],
    "homepage": "https://github.com/ElfSundae/httpclient",
    "license": "MIT",
    "authors": [
        {
            "name": "Elf Sundae",
            "email": "elf.sundae@gmail.com",
            "homepage": "https://0x123.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "~6.3",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.7|~6.0"
    },
    "autoload": {
        "psr-4": {
            "ElfSundae\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElfSundae\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
